<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ehansoo94.planner.dailyboard.dailyBoardMapper">
	<insert id="createDailyBoard">
		insert into MiniProject_01_Planner_#{m_id}_dailyboard values(to_date(#{m_date}, 'DDMMRR'), null, null, null)
	</insert>
	<update id="insertTodoList" parameterType="com.ehansoo94.planner.dailyboard.DailyBoard">
		update MiniProject_01_Planner_#{user }_dailyboard set d_to_do_list = ${d_to_do_list}
	</update>
	<update id="insertSchedule" parameterType="com.ehansoo94.planner.dailyboard.DailyBoard">
		update MiniProject_01_Planner_#{user }_dailyboard set d_scedule = ${d_scedule}
	</update>
	<update id="insertReview" parameterType="com.ehansoo94.planner.dailyboard.DailyBoard">
		update MiniProject_01_Planner_#{user }_dailyboard set d_review = ${d_review}
	</update>
</mapper>